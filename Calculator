Cal
public class Cal {

	public static void main(String[] args) {
		
		int num1=5,num2=10;
		
		calculator obj1=new calculator();
		
		//System.out.println(obj1.sum(num1,num2));
		System.out.println(obj1.sub(num1,num2));
		System.out.println(obj1.mul(num1,num2));
		System.out.println(obj1.div(num1,num2));
		calculator.name();
		System.out.println("Factorial "+obj1.fact(num1));
		
	}

}

calculator
public class calculator {
	
	
	private int sum(int num1, int num2)
	{
		return num1 + num2;
	}
	
	public int sub(int num1, int num2)
	{
		 
		return num1 - num2;
	}
	public int mul(int num1, int num2)
	{
		System.out.println(sum(num1,num2));	 
		return num1 * num2;
	}
	public int div(int num1, int num2)
	{
		return num2 / num1;
	}
	
	public static void name()
	{
		System.out.println("Moiz");
	}
	
	
	public int fact(int num1)
	{		
			int result=1;
			for (int i=1;i<=num1;i++)
			{
			result=result*i;
			}
			
		return result;
		
	}
}
