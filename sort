sorting
public class Sorting {

	public static void main(String[] args) {
		
	int i;
		
		int[] nums = {3, 1, 5, 2, 9, 8};
		 for (int j = 0; j<nums.length; j++) {
		        for (int k = 0; k < nums.length; k++){
		            if (nums[j] < nums[k]) {
		                int x = nums[j];
		                nums[j] = nums[k];
		                nums[k] = x; 
		            }
		        }
		    }
